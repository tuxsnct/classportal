{
	"compilerOptions": {
    "lib": ["es6", "dom"],
		"jsx": "preserve",
    "jsxFactory": "React.createElement",
    "jsxFragmentFactory": "React.Fragment",
    "jsxImportSource": "@emotion/react",
		"moduleResolution": "node",
		"esModuleInterop": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true
  }
}
